{
  "name": "{{pkgName}}",
  "version": "{{applicationVersionWeb}}",
  "genx": {
    "app": {
      "rootElement": "{{rootElement}}"
    },
    "federation": {
      "remotes": [
        "@genesislcap/foundation-zero"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint"
    ],
    "*.styles.ts": [
      "stylelint"
    ]
  },
  "eslintConfig": {
    "extends": "@genesislcap/eslint-config"
  },
  "stylelint": {
    "extends": "@genesislcap/stylelint-config"
  },
  "prettier": "@genesislcap/prettier-config",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:client": "node dist/client/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/ssr": "^17.0.1",
    "@genesislcap/foundation-auth": "14.107.0-auth-mf.27",
    "@genesislcap/foundation-ui": "14.107.0-auth-mf.27",
    "@genesislcap/foundation-zero": "14.107.0-auth-mf.27",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.0",
    "@angular-devkit/build-angular": "^17.0.1",
    "@angular/cli": "^17.0.1",
    "@angular/compiler-cli": "^17.0.0",
    "@genesislcap/build-kit": "14.107.0-auth-mf.27",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "dotenv": "^16.3.1",
    "file-loader": "^6.2.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "style-loader": "^3.3.3",
    "svg-url-loader": "^8.0.0",
    "typescript": "~5.2.2"
  }
}